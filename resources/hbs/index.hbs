{{#partial "content" }}
  {{#each entries}}
    <section class="box">
      <a href="{{name}}">
        <p><small><em>{{created_at}}</em></small></p>
        <h1 class="title">{{title}}</h1>
        <div class="subtitle">{{{description}}}</div>
      </a>
    </section>
  {{/each}}
{{/partial}}

{{> layout}}
